{
  "name": "VII",
  "version": "1.0.0",
  "description": "High Quality Youtube Videos Converter",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build-osx": "electron-packager . \"VII\" --platform=darwin --icon ./src/img/icon.icns --out ./build --overwrite",
    "build-linux": "electron-packager . \"VII\" --platform=linux --icon ./src/img/icon.png --out ./build --overwrite",
    "build-win32": "electron-packager . \"VII\" --platform=win32 --icon ./src/img/icon.ico --out ./build --overwrite",
    "build": "electron-packager . \"VII\" --platform=all --icon ./src/img/icon --out ./build --overwrite"
  },
  "keywords": [
    "Youtube",
    "Downloader",
    "Converter",
    "High Quality",
    "VII"
  ],
  "author": "Arda Gedik <ardagedikk@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.15",
    "bootstrap": "^4.1.1",
    "fluent-ffmpeg": "^2.1.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "ytdl-core": "^0.20.4"
  },
  "devDependencies": {
    "electron": "^2.0.4",
    "electron-packager": "^12.1.0"
  }
}
